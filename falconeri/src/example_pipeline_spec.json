{
  "pipeline": {
    "name": "book_words"
  },
  "transform": {
    "cmd": [
      "python3",
      "/extract_words.py"
    ],
    "image": "somerepo/my_python_nlp"
  },
  "parallelism_spec": {
    "constant": 10
  },
  "resource_requests": {
    "memory": "500Mi",
    "cpu": 1.2
  },
  "node_selector": {
    "node_type": "falconeri_worker"
  },
  "input": {
    "atom": {
      "URI": "gs://example-bucket/books/",
      "repo": "books",
      "glob": "/*"
    }
  },
  "egress": {
    "URI": "gs://example-bucket/words/"
  }
}
